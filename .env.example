# RAG Chatbot Environment Configuration
# Copy to .env and fill in your values

# OpenAI API Key (required for embeddings and agent)
OPENAI_API_KEY=sk-your-openai-api-key

# Qdrant Cloud Configuration
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key
QDRANT_COLLECTION_NAME=physical_ai_textbook

# Neon Postgres Connection (for conversation metadata)
DATABASE_URL=postgresql+asyncpg://user:password@your-neon-host.neon.tech/dbname?sslmode=require

# Backend Configuration
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000
CORS_ORIGINS=http://localhost:3000,https://jannapro.github.io

# Embedding Model
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536

# Agent Model
AGENT_MODEL=gpt-4o-mini

# Content Path (relative to backend/)
CONTENT_PATH=../docs

# Firebase Authentication
# Get these from Firebase Console > Project Settings > Your apps > Web app
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_APP_ID=your-app-id
